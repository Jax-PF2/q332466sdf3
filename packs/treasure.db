{"_id":"ndaJyytjPyg0Vd0i","name":"Galleon Ballista (Artillery)","type":"weapon","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/equipment/weapons/heavy-crossbow.webp","data":{"description":{"value":"<p>This is a great crossbow that fires bolts the size of spears. While not as damaging as a cannon, it does shoot further distances more accurately. Ranged Increment: 10 hexes</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"common","custom":"Ranged 10 hexes"},"rules":[],"slug":"shortbow","schema":{"version":0.768,"lastMigration":{"datetime":"2022-07-28T19:58:19.615-04:00","version":{"schema":0.764,"foundry":"9.269","system":"3.13.1.12"}}},"quantity":1,"baseItem":"crossbow","hp":{"brokenThreshold":0,"value":0,"max":0},"hardness":0,"weight":{"value":"-"},"equippedBulk":{"value":""},"price":{"value":{"gp":3}},"equipped":{"carryType":"worn","invested":null,"handsHeld":0},"stackGroup":null,"negateBulk":{"value":"0"},"containerId":null,"preciousMaterial":{"value":null},"preciousMaterialGrade":{"value":null},"size":"huge","identification":{"status":"identified","unidentified":{"name":"Unusual Shortbow","img":"systems/pf2e/icons/unidentified_item_icons/weapon.webp","data":{"description":{"value":""}}},"misidentified":{}},"usage":{"value":"held-in-two-hands"},"level":{"value":13},"category":"simple","group":"bow","bonus":{"value":0},"damage":{"value":"","dice":3,"die":"d6","damageType":"piercing"},"bonusDamage":{"value":10},"splashDamage":{"value":0},"range":300,"reload":{"value":"0"},"MAP":{"value":"5"},"potencyRune":{"value":0},"strikingRune":{"value":""},"specific":{"value":false},"propertyRune1":{"value":null},"propertyRune2":{"value":null},"propertyRune3":{"value":null},"propertyRune4":{"value":null},"property1":{"value":"","dice":null,"die":"","damageType":"","critDice":null,"critDie":"","critDamage":"","critDamageType":"","criticalConditionType":"","criticalConditionValue":null,"strikeConditionType":"","strikeConditio